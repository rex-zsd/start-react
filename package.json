{
  "name": "start-react",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "better-npm-run dev",
    "test": "better-npm-run test",
    "prod": "better-npm-run prod",
    "lint": "eslint --ext .jsx src"
  },
  "betterScripts": {
    "dev": {
      "command": "webpack-dev-server --config webpack.config.dev.js --colors --profile --display-modules",
      "env": {
        "NODE_ENV": "development",
        "PORT": "9003",
        "HTTPS": true
      }
    },
    "test": {
      "command": "webpack --config ./webpack.config.prod.js",
      "env": {
        "NODE_ENV": "test"
      }
    },
    "prod": {
      "command": "webpack --config ./webpack.config.prod.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rex-zsd/start-react.git"
  },
  "author": "rex-zsd",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/rex-zsd/start-react/issues"
  },
  "homepage": "https://github.com/rex-zsd/start-react#readme",
  "devDependencies": {
    "babel-core": "^6.16.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "better-npm-run": "0.0.13",
    "css-loader": "^0.25.0",
    "file-loader": "^0.9.0",
    "json-loader": "^0.5.4",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "postcss-loader": "^1.1.1",
    "react-router": "^3.0.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack-dev-server": "^1.16.1",
    "webpack-merge": "^0.17.0"
  },
  "dependencies": {
    "autoprefixer": "^6.5.0",
    "babel-polyfill": "^6.16.0",
    "clean-webpack-plugin": "^0.1.11",
    "es5-shim": "^4.5.9",
    "es6-promise": "^4.0.3",
    "html-webpack-plugin": "^2.22.0",
    "isomorphic-fetch": "^2.2.1",
    "normalize.css": "^5.0.0",
    "open-browser-webpack-plugin": "0.0.2",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router-redux": "^4.0.6",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "webpack": "^1.13.2"
  }
