{
  "name": "start-react",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "commit": "git-cz",
    "start": "better-npm-run dev",
    "build:dev": "better-npm-run dev",
    "build:test": "better-npm-run test",
    "build:prod": "better-npm-run prod",
    "lint:js": "eslint --ext .jsx src"
  },
  "betterScripts": {
    "dev": {
      "command": "webpack-dev-server --config build/webpack.config.dev.js",
      "env": {
        "NODE_ENV": "development",
        "PORT": "10003",
        "HTTPS": false
      }
    },
    "test": {
      "command": "webpack --config build/webpack.config.prod.js",
      "env": {
        "NODE_ENV": "test"
      }
    },
    "prod": {
      "command": "webpack --config build/webpack.config.prod.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rex-zsd/start-react.git"
  },
  "author": "rex-zsd",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/rex-zsd/start-react/issues"
  },
  "homepage": "https://github.com/rex-zsd/start-react#readme",
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.40",
    "@babel/preset-env": "^7.0.0-beta.40",
    "@babel/preset-react": "^7.0.0-beta.40",
    "@babel/preset-stage-0": "^7.0.0-beta.40",
    "@babel/runtime": "^7.0.0-beta.40",
    "autoprefixer": "^8.0.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^8.0.0-beta.0",
    "better-npm-run": "^0.1.0",
    "clean-webpack-plugin": "^0.1.11",
    "commitizen": "^2.9.6",
    "conventional-changelog-cli": "^1.3.16",
    "css-loader": "^0.28.1",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^4.17.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-webpack": "^0.8.4",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.6.1",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.22.0",
    "less": "^2.7.1",
    "less-loader": "^4.0.3",
    "postcss-loader": "^2.0.5",
    "style-loader": "^0.20.1",
    "stylelint": "^8.4.0",
    "stylelint-config-standard": "^18.0.0",
    "stylelint-order": "^0.8.0",
    "uglifyjs-webpack-plugin": "^1.1.8",
    "url-loader": "^0.6.2",
    "webpack": "^3.1.0",
    "webpack-dev-server": "^2.4.5",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "core-js": "^2.4.1",
    "isomorphic-fetch": "^2.2.1",
    "normalize.css": "^8.0.0",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-thunk": "^2.1.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
