{
  "name": "start-react",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "better-npm-run dev",
    "dev": "better-npm-run dev",
    "test": "better-npm-run test",
    "prod": "better-npm-run prod",
    "lint": "eslint --ext .jsx src"
  },
  "betterScripts": {
    "dev": {
      "command": "webpack-dev-server --config webpack.config.dev.js",
      "env": {
        "NODE_ENV": "development",
        "PORT": "9003",
        "HTTPS": false
      }
    },
    "test": {
      "command": "webpack --config ./webpack.config.prod.js",
      "env": {
        "NODE_ENV": "test"
      }
    },
    "prod": {
      "command": "webpack --config ./webpack.config.prod.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "repository": {
    "type": "git",
